{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EnvironmentConfiguration": {
    "ActiveSwagger": "{{EnvironmentConfiguration.ActiveSwagger}}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "{{ConnectionStrings.DefaultConnection}}"
  },
  "JwtToken": {
    "JwtKey": "{{JwtToken.JwtKey}}",
    "Issuer": "{{JwtToken.Issuer}}",
    "Audience": "{{JwtToken.Audience}}",
    "DurationInMinutes": "{{JwtToken.DurationInMinutes}}",
    "RequireHttpsMetadata": "{{JwtToken.RequireHttpsMetadata}}"
  },
  "CorsConfiguration": {
    "Web": "{{CorsConfiguration.Web}}",
    "Methods": [
      "{{CorsConfiguration.Methods}}",
      "{{CorsConfiguration.Methods}}",
      "{{CorsConfiguration.Methods}}",
      "{{CorsConfiguration.Methods}}",
      "{{CorsConfiguration.Methods}}",
      "{{CorsConfiguration.Methods}}"
    ]
  },
  "EmailSettings": {
    "SenderName": "{{EmailSettings.SenderName}}",
    "SenderEmail": "{{EmailSettings.SenderEmail}}",
    "Password": "{{EmailSettings.Password}}",
    "ServerAddress": "{{EmailSettings.ServerAddress}}",
    "Port": "{{EmailSettings.Port}}",
    "Ssl": "{{EmailSettings.Ssl}}",
    "Cc": [
      "{{EmailSettings.Cc}}",
      "{{EmailSettings.Cc}}"
    ]
  }
}